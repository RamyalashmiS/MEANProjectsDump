{
    "env": {
        "node": true
    },
   "extends": "eslint:recommended",
   "parser":"babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6
        
    },
    "rules": {
        "camelcase": "warn",
        "eqeqeq": "error",
        "no-console": "warn",
        "no-empty-function": "error",
        "quotes": [
            "warn",
            "double"
        ],
        "require-await": "error",
        "semi-style": [
            "warn",
            "last"
        ],
        "no-var": "error",
        "no-unused-vars": "warn",
        "no-undef": "error"
    }
}